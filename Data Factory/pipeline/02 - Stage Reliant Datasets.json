{
	"name": "03 - Stage Reliant Datasets",
	"properties": {
		"activities": [
			{
				"name": "Get File Name List",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"dataset": {
						"referenceName": "datalake",
						"type": "DatasetReference",
						"parameters": {
							"container": {
								"value": "@pipeline().globalParameters.bronze_containername",
								"type": "Expression"
							},
							"fileName": {
								"value": "@pipeline().parameters.directory",
								"type": "Expression"
							}
						}
					}
				}
			}
		],
		"parameters": {
			"directory": {
				"type": "string"
			}
		},
		"variables": {
			"fileNames": {
				"type": "Array"
			}
		},
		"folder": {
			"name": "Bronze"
		},
		"annotations": []
	}
}